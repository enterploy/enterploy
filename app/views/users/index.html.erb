

<div >
		<%= link_to "Main Page", root_path %>
</div>


<% @user = User.all %>

<% @user.each do|user|%>

	<% if user.user_kind == 'Employee Seeker' %>
		<div class= "well">
			Name: <%= user.first_name%><br>
			Type: <%= user.user_kind%><br>
			<%= link_to 'Add This Company', new_user_app_path(app_id: user), class: 'btn btn-primary' %>
		</div>
	<%end %>
<% end %>

