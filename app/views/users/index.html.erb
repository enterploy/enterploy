<div >
		<%= link_to "Main Page", root_path %>
</div>


<% @user = User.all %>

<% @user.each do|u|%>

	<% if u.user_kind == 'Employee Seeker' %>
		<div class= "well">
			Name: <%= u.first_name%><br>
			Type: <%= u.user_kind%><br>
			<%= link_to 'Add This Company', new_user_app_path(app_id: u), class: 'btn btn-primary' %>
		</div>
	<%end %>
<% end %>

