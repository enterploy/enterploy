<%= simple_form_for(@job_listing, html: {class: "well"}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :company_name %>
  	<%= f.input :company_city %>
  	<%= f.input :company_state %>
  	<%= f.input :job_type %>
  	<%= f.input :job_title %>
  	<%= f.input :job_description %>
  	<%= f.input :job_responsibilities %>
  	<%= f.input :job_benefits %>
  	<%= f.input :pay_range %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

